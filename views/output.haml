.row
	.six.columns.centered
		%h4 File
		%table
			%tbody
				%tr
					%td Name
					%td= @file[:filename]
				%tr
					%td Type
					%td= @file[:type]
				%tr
					%td Size
					%td= @file[:size]

		%h4 Tracks
		%table
			%tbody
			- @ableton[:Tracks].each do |track|
				%tr
					%td Track
					%td= track
			%tr
				%td Master Track
				%td= @ableton[:MasterTrack]